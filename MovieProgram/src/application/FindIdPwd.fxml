<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<BorderPane fx:id="FindIdPwd" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.FindIdPwdController">
	<center>
		<VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0"
			spacing="50.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="btnId" onAction="#handleBtnId"
					mnemonicParsing="false" prefHeight="61.0" prefWidth="190.0"
					text="ID찾기">
					<font>
						<Font size="33.0" />
					</font>
				</Button>
				<Button fx:id="btnPwd" mnemonicParsing="false" text="PWD찾기">
					<font>
						<Font size="33.0" />
					</font>
				</Button>
			</children>
		</VBox>
	</center>
	<top>
		<Text strokeType="OUTSIDE" strokeWidth="0.0" text="ID/PWD 찾기"
			BorderPane.alignment="CENTER">
			<font>
				<Font size="41.0" />
			</font>
			<BorderPane.margin>
				<Insets top="30.0" />
			</BorderPane.margin>
		</Text>
	</top>
	<bottom>
		<Button fx:id="btnCancle" onAction="#handleBtnCancle"
			mnemonicParsing="false" text="취소" BorderPane.alignment="CENTER">
			<font>
				<Font size="27.0" />
			</font>
			<BorderPane.margin>
				<Insets bottom="30.0" />
			</BorderPane.margin>
		</Button>
	</bottom>
</BorderPane>
