<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="signUpComplete" prefHeight="400.0"
	prefWidth="500.0" xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.SignUpCompleteController">
	<center>
		<Text strokeType="OUTSIDE" strokeWidth="0.0" text="회원가입 완료!"
			BorderPane.alignment="CENTER">
			<font>
				<Font size="43.0" />
			</font>
		</Text>
	</center>
	<bottom>
		<Button fx:id="btnCommit" mnemonicParsing="false"
			onAction="#handleBtnAddAccountAction" text="로그인 하러가기"
			BorderPane.alignment="CENTER">
			<font>
				<Font size="33.0" />
			</font>
			<BorderPane.margin>
				<Insets bottom="50.0" />
			</BorderPane.margin>
		</Button>
	</bottom>
</BorderPane>
